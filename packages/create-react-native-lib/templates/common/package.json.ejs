{
  "name": "react-native-<%= options.name %>",
  "version": "0.0.0",
  "description": "",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "react-native": "src/index.tsx",
  "types": "lib/typescript/src/index.d.ts",
  "source": "src/index.tsx",
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "scripts": {
<%_ if (options.usesJest) { -%>
    "test": "jest",
<%_ } -%>
<%_ if (options.usesLinter) { -%>
    "lint": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx",
<%_ } -%>
    "prepare": "bob build"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
<%_ if (options.reactVersion === "18") { -%>
    "react": ">=18.2.0",
    "react-native": ">=0.73.3"
<%_ } else { -%>
    "react": ">=17.0.2",
    "react-native": ">=0.68.7"
<%_ } -%>
  },
  "dependencies": {
<%_ if (options.usesReactNativeSvg) { -%>
    "react-native-svg": "14.1.0"
<%_ } -%>
  },
  "devDependencies": {
    "@types/node": "20.11.16",
<%_ if (options.reactVersion === "18") { -%>
    "@types/react": "18.2.52",
    "react": "18.2.0",
    "react-native": "0.73.3",
<%_ } else { -%>
    "@types/react": "17.0.74",
    "@types/react-native": "0.68.20",
    "react": "17.0.2",
    "react-native": "0.68.7",
<%_ } -%>
<%_ if (options.usesLinter) { -%>
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    <%_ if (options.linterType === "standard") { -%>
    "eslint-config-standard-with-typescript": "42.0.0",
    <%_ } -%>
    <%_ if (options.linterType === "airbnb") { -%>
    "eslint-config-airbnb-typescript": "17.1.0",
    <%_ } -%>
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "46.10.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "3.1.1",
<%_ } -%>
<%_ if (options.usesJest) { -%>
    "jest": "29.7.0",
    "@types/jest": "29.5.11",
    "babel-jest": "29.7.0",
    "@testing-library/jest-native": "5.4.3",
    "@testing-library/react-native": "12.4.3",
    "metro-react-native-babel-preset": "0.77.0",
    "identity-obj-proxy": "3.0.0",
  <%_ if (options.reactVersion === "18") { -%>
    "@types/react-test-renderer": "18.0.7",    
    "react-test-renderer": "18.2.0",
  <%_ } else { -%>
    "@testing-library/react": "12.1.5",
    "react-test-renderer": "17.0.2",
  <%_ } -%>
<%_ } -%>
    "react-native-builder-bob": "0.23.2",
    "typescript": "5.3.3"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "exclude": "**/*.{test,stories}.tsx",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
