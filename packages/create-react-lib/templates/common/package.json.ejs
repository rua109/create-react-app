{
  "name": "<%= options.name %>",
  "version": "0.0.0",
  "description": "",
  "main": "dist/module/main.bundle.js",
  "types": "dist/typescript/index.d.ts",
  "scripts": {
<%_ if (options.usesStorybook) { -%>
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
<%_ } -%>
<%_ if (options.usesJest) { -%>
    "test": "jest",
<%_ } -%>
<%_ if (options.usesLinter) { -%>
    "lint": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx",
<%_ } -%>
    "build": "webpack --config webpack.config.js && tsc --declaration --emitDeclarationOnly --declarationDir ./dist/typescript"
  },
  "author": "",
  "license": "ISC",
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
<%_ if (options.usesStorybook) { -%>
    "@storybook/addon-essentials": "7.6.9",
    "@storybook/addon-interactions": "7.6.9",
    "@storybook/addon-links": "7.6.9",
    "@storybook/addon-styling-webpack": "0.0.5",
    "@storybook/addon-themes": "7.6.9",
    "@storybook/blocks": "7.6.9",
    "@storybook/react": "7.6.9",
    "@storybook/react-webpack5": "7.6.9",
    "@storybook/test": "7.6.9",
  <%_ if (options.usesLinter) { -%>
    "eslint-plugin-storybook": "0.6.15",
  <%_ } -%>
    "storybook": "7.6.9",
<%_ } -%>
<%_ if (options.transpiler === "swc") { -%>
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.104",
    "swc-loader": "0.2.3",
  <%_ if (options.usesJest) { -%>
    "@swc/jest": "0.2.30",
  <%_ } -%>
<%_ } else { -%>
    "@babel/core": "7.23.7",
    "@babel/plugin-transform-runtime": "7.23.7",
    "@babel/preset-env": "7.23.8",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "babel-loader": "9.1.3",
  <%_ if (options.usesJest) { -%>
    "babel-jest": "29.7.0",
  <%_ } -%>
<%_ } -%>
    "typescript": "^5.3.3",
<%_ if (options.usesLinter) { -%>
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    <%_ if (options.linterType === "standard") { -%>
    "eslint-config-standard-with-typescript": "42.0.0",
    <%_ } -%>
    <%_ if (options.linterType === "airbnb") { -%>
    "eslint-config-airbnb-typescript": "17.1.0",
    <%_ } -%>
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "46.10.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    <%_ if (options.usesReact) { -%>
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    <%_ } else { -%>
    "@tsconfig/strictest": "2.0.2",
    <%_ } -%>
    "prettier": "3.1.1",
<%_ } -%>
<%_ if (options.usesJest) { -%>
    "@types/jest": "29.5.11",
    "jest": "29.7.0",
<%_ } -%>
<%_ if (options.usesReact) { -%>
    "@tsconfig/create-react-app": "2.0.2",
    "@types/node": "20.11.5",
    "css-loader": "6.9.0",
    "mini-css-extract-plugin": "2.7.7",
    "css-minimizer-webpack-plugin": "5.0.1",
  <%_ if (options.usesJest) { -%>
    "@testing-library/jest-dom": "6.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest-environment-jsdom": "29.7.0",
  <%_ } -%>
<%_ } -%>
<%_ if (options.reactVersion === "18") { -%>
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@types/react": "18.2.48",
  <%_ if (options.usesJest) { -%>
    "@testing-library/react": "14.1.2",
    "react-test-renderer": "18.2.0",
  <%_ } -%>
<%_ } else { -%>
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "@types/react": "17.0.75",
  <%_ if (options.usesJest) { -%>
    "@testing-library/react": "12.1.5",
    "react-test-renderer": "17.0.2",
  <%_ } -%>
<%_ } -%>
<%_ if (options.usesTailwind) { -%>
    "autoprefixer": "10.4.17",
    "postcss": "8.4.33",
    "postcss-loader": "7.3.4",
    "postcss-nesting": "12.0.2",
    "tailwindcss": "3.4.1",
<%_ } -%>
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "clean-webpack-plugin": "4.0.0"
  },
  "dependencies": {
<%_ if (options.transpiler === "babel") { -%>
    "@babel/runtime": "7.23.8"
<%_ } -%>
  },
  "peerDependencies": {
<%_ if (options.reactVersion === "18") { -%>
    "react": ">=18"
<%_ } else { -%>
    "react": ">=17"
<%_ } -%>
  }
}
