{
  "name": "<%= options.name %>",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "webpack-dev-server  --config webpack.dev.js --open",
    "build": "webpack --config webpack.prod.js",
    "test": "jest",
    "lint": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx",
    <%_ if (options.usesStorybook) { -%>
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    <%_ } -%>
    "pub": "npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rua109/create-react-app-swc.git"
  },
  "author": "rua109",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rua109/create-react-app-swc/issues"
  },
  "homepage": "https://github.com/rua109/create-react-app-swc#readme",
  "bin": "./bin/cli.js",
  "devDependencies": {
    <%_ if (options.usesStorybook) { -%>
    "@storybook/addon-essentials": "7.6.9",
    "@storybook/addon-interactions": "7.6.9",
    "@storybook/addon-links": "7.6.9",
    "@storybook/addon-styling-webpack": "0.0.5",
    "@storybook/addon-themes": "7.6.9",
    "@storybook/blocks": "7.6.9",
    "@storybook/react": "7.6.9",
    "@storybook/react-webpack5": "7.6.9",
    "@storybook/test": "7.6.9",
    "eslint-plugin-storybook": "0.6.15",
    "storybook": "7.6.9",
    <%_ } -%>
    <%_ if (options.transpiler === "swc") { -%>
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.104",
    "@swc/helpers": "0.5.3",
    "@swc/jest": "0.2.30",
    "swc-loader": "0.2.3",
    <%_ } else { -%>
    "@babel/core": "7.23.7",
    "@babel/plugin-transform-runtime": "7.23.7",
    "@babel/preset-env": "7.23.8",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "babel-loader": "9.1.3",
    "babel-jest": "29.7.0",
    <%_ } -%>
    "@testing-library/jest-dom": "6.2.0",
    <%_ if (options.reactVersion === "18") { -%>
    "@testing-library/react": "14.1.2",
    <%_ } else { -%>
    "@testing-library/react": "12.1.5",
    <%_ } -%>
    "@tsconfig/create-react-app": "2.0.2",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "6.9.0",
    "css-minimizer-webpack-plugin": "5.0.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "42.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "46.10.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "html-webpack-plugin": "5.6.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "mini-css-extract-plugin": "2.7.7",
    "prettier": "3.1.1",
    <%_ if (options.reactVersion === "18") { -%>
    "react-test-renderer": "18.2.0",
    <%_ } else { -%>
    "react-test-renderer": "17.0.2",
    <%_ } -%>
    "style-loader": "3.3.4",
    <%_ if (options.usesTailwind) { -%>
    "autoprefixer": "10.4.17",
    "postcss": "8.4.33",
    "postcss-loader": "7.3.4",
    "postcss-nesting": "12.0.2",
    "tailwindcss": "3.4.1",
    <%_ } -%>
    "typescript": "5.3.3",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "webpack-merge": "5.10.0"
  },
  "dependencies": {
    <%_ if (options.transpiler === "babel") { -%>
    "@babel/runtime": "7.23.8",
    <%_ } -%>
    <%_ if (options.reactVersion === "18") { -%>
    "react": "18.2.0",
    "react-dom": "18.2.0"
    <%_ } else { -%>
    "react": "17.0.2",
    "react-dom": "17.0.2"
    <%_ } -%>
  }
}
